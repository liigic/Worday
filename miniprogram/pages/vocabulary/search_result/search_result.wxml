<view class="container" >
  <view class="header">
    <text class='header-desc'>Remember words every day.</text>
  </view>
  <l-card l-class="card" type="primary" plaintext="{{true}}" >
    <view class="word-container">
      <view class="word-head">headword</view>
      <view class="word-trans">
        <view class="word-trans-title">释义</view>
        <view class="word-trans-content">n.释义</view>
      </view>
    </view>
  </l-card>
  <view class="btn-box">
    <l-button l-class="l-button" type="default" width="230" height="92" shape="circle" bg-color="#2c3c65" bind:lintap="backHome">返回主页</l-button>
  </view>
</view>
